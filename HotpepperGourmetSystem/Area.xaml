<Page x:Class="HotpepperGourmetSystem.Area"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:HotpepperGourmetSystem"
      mc:Ignorable="d" 
      Title="Home" Height="650" Width="1000">
    <StackPanel>
        <Grid Height="626" HorizontalAlignment="Left" Width="1000" Background="#FFEEEEEE">
            <Grid.RowDefinitions>
                <RowDefinition Height="201"/>
                <RowDefinition Height="68"/>
                <RowDefinition Height="42"/>
                <RowDefinition Height="42"/>
                <RowDefinition Height="42"/>
                <RowDefinition Height="42"/>
                <RowDefinition Height="189"/>
            </Grid.RowDefinitions>
            <!--<Image HorizontalAlignment="Left" Height="701" Margin="0,0,0,-75" VerticalAlignment="Top" Width="995" Source="Images/Area背景.png" Grid.RowSpan="7"/>-->
            <Image HorizontalAlignment="Left" Height="701" Margin="0,0,0,-75" VerticalAlignment="Top" Width="995" Source="Images/背景Area.png" Grid.RowSpan="7"/>
            <Image HorizontalAlignment="Left" Height="50" Margin="31,32,0,0" VerticalAlignment="Top" Width="132" Source="Images/Hotpepper.jpg"/>
            <Image HorizontalAlignment="Left" Height="498" Margin="245,66,0,0" VerticalAlignment="Top" Width="476" Source="Images/日本地図1.png" Grid.RowSpan="7"/>
            <TextBlock HorizontalAlignment="Left" Margin="44,10,0,0" Grid.Row="1" TextWrapping="Wrap" Text="── 地域 ──" VerticalAlignment="Top" Height="22" Width="108" FontSize="16" Foreground="White"/>
            <Button x:Name="goHome" Content="HOME" HorizontalAlignment="Left" Margin="31,10,0,0" Grid.Row="2" VerticalAlignment="Top" Width="138" Background="#FF252525" BorderBrush="{x:Null}" Click="goHome_Click"/>
            <Button x:Name="goGenre" Content="ジャンル" HorizontalAlignment="Left" Margin="31,10,0,0" Grid.Row="3" VerticalAlignment="Top" Width="138" Background="#FF252525" BorderBrush="{x:Null}" Click="goGenre_Click"/>
            <Button x:Name="goPicky" Content="こだわり" HorizontalAlignment="Left" Margin="31,10,0,0" Grid.Row="4" VerticalAlignment="Top" Width="138" Background="#FF252525" BorderBrush="{x:Null}" Click="goPicky_Click"/>
            <Button x:Name="goHistory" Content="★" HorizontalAlignment="Left" Margin="31,10,0,0" Grid.Row="5" VerticalAlignment="Top" Width="138" Background="#FF252525" BorderBrush="{x:Null}" Click="goHistory_Click"/>
            <Button Content="北海道" HorizontalAlignment="Left" Margin="624,84,0,0" VerticalAlignment="Top" Width="80" Background="#FFD3A2D0" Height="28" Foreground="#DDFFFFFF" BorderBrush="{x:Null}"/>
            <Button Content="青森" HorizontalAlignment="Left" Margin="619,13,0,0" VerticalAlignment="Top" Width="66" Background="#FF82B0DE" Height="28" Grid.Row="1" BorderBrush="{x:Null}"/>
            <Button Content="岩手" HorizontalAlignment="Left" Margin="690,13,0,0" Grid.Row="1" VerticalAlignment="Top" Width="66" Height="28" Background="#FF82B0DE" BorderBrush="{x:Null}"/>
            <Button Content="秋田" HorizontalAlignment="Left" Margin="619,48,0,0" Grid.Row="1" VerticalAlignment="Top" Width="66" Height="28" Grid.RowSpan="2" Background="#FF82B0DE" BorderBrush="{x:Null}"/>
            <Button Content="宮城" HorizontalAlignment="Left" Margin="690,48,0,0" Grid.Row="1" VerticalAlignment="Top" Width="66" Height="28" Grid.RowSpan="2" BorderBrush="{x:Null}" Background="#FF82B0DE"/>
            <Button Content="山形" HorizontalAlignment="Left" Margin="619,13,0,0" Grid.Row="2" VerticalAlignment="Top" Width="66" Height="30" Background="#FF82B0DE" BorderBrush="{x:Null}" Grid.RowSpan="2"/>
            <Button Content="福島" HorizontalAlignment="Left" Margin="690,13,0,0" Grid.Row="2" VerticalAlignment="Top" Width="66" Height="30" Background="#FF82B0DE" BorderBrush="{x:Null}" Grid.RowSpan="2"/>
            <Button Content="東京" HorizontalAlignment="Left" Margin="588,13,0,0" Grid.Row="3" VerticalAlignment="Top" Width="66" Height="29" Background="#FFA8DBEA" BorderBrush="{x:Null}"/>
            <Button Content="神奈川" HorizontalAlignment="Left" Margin="659,13,0,0" VerticalAlignment="Top" Width="80" Height="29" Grid.Row="3" Background="#FFA8DBEA" BorderBrush="{x:Null}"/>
            <Button Content="埼玉" HorizontalAlignment="Left" Margin="588,5,0,0" Grid.Row="4" VerticalAlignment="Top" Width="66" Height="29" Background="#FFA8DBEA" BorderBrush="{x:Null}"/>
            <Button Content="千葉" HorizontalAlignment="Left" Margin="659,5,0,0" Grid.Row="4" VerticalAlignment="Top" Width="66" Height="29" Background="#FFA8DBEA" BorderBrush="{x:Null}"/>
            <Button Content="茨城" HorizontalAlignment="Left" Margin="549,39,0,0" Grid.Row="4" VerticalAlignment="Top" Width="66" Height="29" Grid.RowSpan="2" BorderBrush="{x:Null}" Background="#FFA8DBEA"/>
            <Button Content="栃木" HorizontalAlignment="Left" Margin="620,39,0,0" Grid.Row="4" VerticalAlignment="Top" Width="66" Grid.RowSpan="2" Height="29" BorderBrush="{x:Null}" Background="#FFA8DBEA"/>
            <Button Content="群馬" HorizontalAlignment="Left" Margin="691,39,0,0" Grid.Row="4" Grid.RowSpan="2" VerticalAlignment="Top" Width="66" Height="29" Background="#FFA8DBEA" BorderBrush="{x:Null}"/>
            <Button Content="沖縄" HorizontalAlignment="Left" Margin="630,57,0,0" Grid.Row="6" VerticalAlignment="Top" Width="66" Height="28" Background="#FFEC817C" BorderBrush="{x:Null}"/>
            <Button Content="新潟" HorizontalAlignment="Left" Margin="371,156,0,0" VerticalAlignment="Top" Width="66" Height="28" BorderBrush="{x:Null}" Background="#FF8BD1AE"/>
            <Button Content="長野" HorizontalAlignment="Left" Margin="442,156,0,0" VerticalAlignment="Top" Width="66" Height="28" Background="#FF8BD1AE" BorderBrush="{x:Null}"/>
            <Button Content="山梨" HorizontalAlignment="Left" Margin="371,189,0,0" VerticalAlignment="Top" Width="66" Height="28" Grid.RowSpan="2" Background="#FF8BD1AE" BorderBrush="{x:Null}"/>
            <Button Content="富山" HorizontalAlignment="Left" Margin="442,189,0,0" VerticalAlignment="Top" Width="66" Grid.RowSpan="2" Height="28" Background="#FF8BD1AE" BorderBrush="{x:Null}"/>
            <Button Content="石川" HorizontalAlignment="Left" Margin="371,21,0,0" VerticalAlignment="Top" Width="66" Height="28" Grid.Row="1" Background="#FF8BD1AE" BorderBrush="{x:Null}"/>
            <Button Content="福井" HorizontalAlignment="Left" Margin="442,21,0,0" Grid.Row="1" VerticalAlignment="Top" Width="66" Height="28" Background="#FF8BD1AE" BorderBrush="{x:Null}"/>
            <Button Content="愛知" HorizontalAlignment="Left" Margin="468,40,0,0" Grid.Row="5" VerticalAlignment="Top" Width="66" Background="#FF8BD1AE" BorderBrush="{x:Null}" Height="28" Grid.RowSpan="2"/>
            <Button Content="岐阜" HorizontalAlignment="Left" Margin="539,40,0,0" Grid.Row="5" VerticalAlignment="Top" Width="66" Height="28" Grid.RowSpan="2" BorderBrush="{x:Null}" Background="#FF8BD1AE"/>
        </Grid>
    </StackPanel>
</Page>
